from PycharmProjects.TestPy.ddoamp06 import count

dictCal = {"1":[-10,3],"2":[-1,10],"3":[4,15],"4":[9,16] ,"5":[12,27] ,"6":[16,30] ,"7":[22,42] ,"8":[24,43]
            ,"9":[20,35],"10":[18,33],"11":[10,21],"12":[-3,15]
             }



print("Q1")# 1년 평균 기온차
total =0
for low,high in dictCal.values():
    total += high - low

print( total /len(dictCal.keys()))
# 틀릴리가없는데

print("=====================")

# 기온차 가장 큰 달 - 작은 달 -> 언제이고 기온차는 얼마인가
print(type(dictCal))
print("Q2")
gaptem = sorted(dictCal.items(), key=lambda x:x[1][1] - x[1][0], reverse=True)  # 최고기온 - 최저기온
# .'. x[1][1] == 최고기온   x[1][0] == 최저기온
print(gaptem)

for month, temps in gaptem:
    month, temps, "기온차:", temps[1] - temps[0]
print("=====================")
# 기온차 가장 큰 달
gapMax = gaptem[0][1][1] - gaptem[0][1][0]
print("기온차 가장 큰 달:", gaptem[0][0][0], "기온차:", gapMax)
print(gaptem[0])
print(gaptem[0][0])
print(gaptem[0][0][0])
print("=====================")
print("Q3")


#3번째로 큰 값
month_list = []
temps_list = []
# 빈리스트를 만들었다면? append 
for month, temps in gaptem:
    month_list.append(month)
    temps_list.append(temps)
gaprd = gaptem[1][1][1] - gaptem[1][1][0]
# 3 = list [2]
print(f"{month_list[2]} 월")
print(f"기온차 : {abs(temps_list[2][1] - temps_list[2][0])}")

